{<br/>
  SIM900.println(“AT+CNMI=2,2,0,0,0”); // AT Command to receive a live SMS<br/>
  delay(1000);<br/>
  if (inchar==’ON_LED’)<br/>
  {<br/>
   digitalWrite(led, HIGH);<br/>
  }<br/>
  else if (inchar==’OFF_LED’)<br/>
