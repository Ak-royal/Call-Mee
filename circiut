boolean bIsRaining = false;<br/>
String strRaining;</p>
<p>void setup() {<br/>
  mySerial.begin(9600);<br/>
  Serial.begin(9600);<br/>
  pinMode(2,INPUT);<br/>
  pinMode(gsm,OUTPUT);<br/>
}<br/>
void loop() {<br/>
  nRainVal = analogRead(nRainIn);<br/>
  bIsRaining = !(digitalRead(nRainDigitalIn));</p>
