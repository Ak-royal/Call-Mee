  else{<br/>
    strRaining = “NO”;<br/>
    digitalWrite(gsm,LOW);<br/>
  }</p>
<p>  Serial.print(“Raining?: “);<br/>
  Serial.print(strRaining);<br/>
  Serial.print(“\t Moisture Level: “);<br/>
  Serial.println(nRainVal);</p>
